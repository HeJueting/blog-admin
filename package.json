{
  "name": "blog-admin-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "antd": "^4.10.3",
    "axios": "^0.21.1",
    "braft-editor": "^2.3.9",
    "braft-extensions": "^0.1.1",
    "draft-js-prism": "^1.0.6",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "prismjs": "^1.23.0",
    "react": "^16.13.1",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^16.13.1",
    "react-json-view": "^1.21.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .tsx,.ts src/",
    "analyze": "source-map-explorer build/static/js/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.2",
    "typescript-plugin-css-modules": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "eslint --fix --ext .tsx,.ts src/",
      "git add"
    ]
  }
}
